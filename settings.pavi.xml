<?xml version="1.0"?>
<pecan>
  <pfts>
    <pft>
      <name>ebifarm.c4crop</name>
      <outdir>~/pecan/out/pavi/</outdir>
      <edin>~/pecan/edin/ebifarm/aED2IN</edin>
      <exclude>
        <trait>seedling_mortality</trait>    
      </exclude>
      <constants>
        <num>15</num>
        <phenology>2</phenology>
        <max_dbh>0.78</max_dbh>
        <hgt_min>2.0</hgt_min>
        <dark_respiration_factor>0.015</dark_respiration_factor>
        <qsw>0.0</qsw>
        <mort1>1.0</mort1>
        <plant_min_temp>-100</plant_min_temp>
        <storage_turnover_rate>0</storage_turnover_rate>
      </constants>
    </pft>
  </pfts>
  
  <config.header>
    <radiation>
      <lai_min>0.01</lai_min>
    </radiation>
    <ed_misc>
      <output_month>12</output_month>      
    </ed_misc> 
  </config.header>
  
  <meta.analysis>
    <iter>50000</iter>
    <random.effects>TRUE</random.effects>
  </meta.analysis>
  
  <ensemble>
    <size>100</size>
  </ensemble>
  
  <sensitivity.analysis>
    <quantiles>
      <quantile></quantile>
      <sigma>-3</sigma>
      <sigma>-2</sigma>
      <sigma>-1</sigma>
      <sigma>1</sigma>
      <sigma>2</sigma>
      <sigma>3</sigma>
    </quantiles>
    <start.year></start.year>
    <end.year></end.year>
  </sensitivity.analysis>
  
  <pecanDir>~/pecan/</pecanDir>
  <Rlibdir>~/lib/R/</Rlibdir>
  <outdir>~/pecan/out/</outdir>
  <database>
    <userid>ebi_analys_user</userid>
    <passwd>b742xsAu</passwd>
    <location>localhost</location>
    <name>ebi_analysis</name>
  </database>
  
  <run>
    <site>
      <lat></lat>
      <lon></lon>
      <met.data.header></met.data.header>
    </site>
    <start.date></start.date>
    <end.date></end.date>
    <host>
      <name>ebi-cluster.igb.uiuc.edu</name>
      <rundir>~/EDBRAMS/ED/run/</rundir>
      <outdir>/home/scratch/$USER/pecan/out/</outdir>
    </host>
  </run>
</pecan>

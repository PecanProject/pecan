<?xml version="1.0"?>
<pecan>
  <pfts>
    <pft>
      <name>ebifarm.pavi</name>
      <outdir>/home/dlebauer/pecan/out/pavi/</outdir>
      <constants>
        <num>15</num>
        <phenology>2</phenology>
        <max_dbh>0.78</max_dbh>
        <hgt_min>2.0</hgt_min>
        <dark_respiration_factor>0.015</dark_respiration_factor>
        <qsw>0.0</qsw>
        <mort1>1.0</mort1>
        <plant_min_temp>-100</plant_min_temp>
        <storage_turnover_rate>0</storage_turnover_rate>
      </constants>
    </pft>
  </pfts>
  
  <config.header>
    <radiation>
      <lai_min>0.01</lai_min>
    </radiation>
    <ed_misc>
      <output_month>12</output_month>      
    </ed_misc> 
  </config.header>
  
  <meta.analysis>
    <iter>20000</iter>
    <random.effects>TRUE</random.effects>
  </meta.analysis>
  
  <ensemble>
    <size>10</size>
  </ensemble>
  
  <sensitivity.analysis>
    <quantiles>
      <quantile></quantile>
      <sigma>-3</sigma>
      <sigma>-2</sigma>
      <sigma>-1</sigma>
      <sigma>1</sigma>
      <sigma>2</sigma>
      <sigma>3</sigma>
    </quantiles>
    <start.year>2005</start.year>
    <end.year>2006</end.year>
  </sensitivity.analysis>

  <data.assimilation>
    <nmcmc>1000</nmcmc>
  </data.assimilation>
      
  <pecanDir>~/pecan/</pecanDir>
  <Rlibdir>~/lib/R/</Rlibdir>
  <outdir>~/pecan/out/</outdir>
  <database>
    <name>ebi_analysis</name>
    <userid>ebi_analys_user</userid>
    <passwd>b742xsAu</passwd>
    <location>localhost</location>
  </database>
  
  <run>
    <site>
      <name>ebi.farm</name>
      <lat></lat>
      <lon></lon>
      <met.data.header></met.data.header>
    </site>
    <start.date>2005/01/01</start.date>
    <end.date>2006/12/31</end.date>
    <host>
      <name>ebi-cluster.igb.uiuc.edu</name>
      <rundir>/home/dlebauer/EDBRAMS/ED/run/</rundir>
      <inputs>/home/scratch/dlebauer/pecan/edin/</inputs>
      <outdir>/home/scratch/dlebauer/pecan/out/test1/</outdir>
    </host>
    <edin>/home/dlebauer/pecan/edin/templateED2IN-ebifarm.pavi</edin>
  </run>
</pecan>

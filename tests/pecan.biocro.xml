<?xml version="1.0"?>
<pecan>
  <outdir>pecan</outdir>
  <pfts>
    <pft>
      <name>biocro.salix</name>
    </pft>
  </pfts>
  <database>
    <username>bety</username>
    <password>bety</password>
    <dbname>bety</dbname>
    <host>localhost</host>
  </database>
  <meta.analysis>
    <iter>3000</iter>
    <random.effects>FALSE</random.effects>
    <update>TRUE</update>
  </meta.analysis>
  <ensemble>
    <size>3</size>
  </ensemble>
  <sensitivity.analysis>
    <quantiles>
      <sigma>1</sigma>
      <sigma>0</sigma>
      <sigma>-1</sigma>
    </quantiles>
    <variable>StemBiom</variable>
    <start.year>2004</start.year>
    <end.year>2004</end.year>
  </sensitivity.analysis>
  <model>
    <name>BIOCRO</name>
  </model>
  <run>
    <site>
      <id>288</id>
      <name>Sapelo Island</name>
      <lat>31.40</lat>
      <lon>-81.37</lon>
      <met>NULL</met>
    </site>
    <folder>/tmp/</folder>
    <start.date>2004-01-01 06:00:00</start.date>
    <end.date>2005-12-31 05:59:00</end.date>
    <host> 
      <name>localhost</name>
    </host>
  </run>
</pecan>

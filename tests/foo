      <name>sipnet.temperate.coniferous</name> 
    </pft>
  </pfts>

  <ensemble>
    <variable>NPP</variable>
  </ensemble>

  <sensitivity.analysis>
    <quantiles>
      <sigma>-1</sigma>
      <sigma>1</sigma>
    </quantiles>
    <variable>NPP</variable>
  </sensitivity.analysis>

  <model>
    <binary>/usr/local/bin/sipnet.runk</binary>
    <name>SIPNET</name>
  </model>

  <run>
    <site>
      <name>Niwot Ridge Forest/LTER NWT1 (US-NR1)</name>

  leaflong 0.5				! Leaf longevity (years)
  turnover_leaf 1
  turnover_root 0.7		! 0.5 hos guess2008
  !cton_leaf_min 16
  cton_root 29
  nuptoroot 0.00551
  km_volume 0.000001876
  fnstorage 0.3
  litterme 0.2

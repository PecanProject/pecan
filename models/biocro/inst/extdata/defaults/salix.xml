    <kLeaf2>0.98</kLeaf2>
    <kRoot2>0.005</kRoot2>
    <kRhizome2>0.005</kRhizome2>
    <kStem3>0.5</kStem3>
    <kLeaf3>0.15</kLeaf3>
    <kRoot3>0.175</kRoot3>
    <kRhizome3>0.175</kRhizome3>
    <kStem4>0.7</kStem4>
    <kLeaf4>0.15</kLeaf4>
    <kRoot4>0.075</kRoot4>
    <kRhizome4>0.075</kRhizome4>

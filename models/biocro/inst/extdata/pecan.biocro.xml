<?xml version="1.0"?>
<pecan>
  <outdir>/tmp/biocrotest/</outdir>
  <pfts>
    <pft>
      <name>biocro.salix</name>
    </pft>
  </pfts>
  <database>
    <user>bety</user>
    <password>bety</password>
    <dbname>bety</dbname>
    <host>localhost</host>
    <dbfiles>$HOME/.pecan/dbfiles</dbfiles>
  </database>
  <meta.analysis>
    <iter>3000</iter>
    <random.effects>FALSE</random.effects>
  </meta.analysis>
  <ensemble>
    <size>1</size>
    <variable>StemBiom</variable>
  </ensemble>
  <sensitivity.analysis/>
  <model>
    <name>BIOCRO</name>
  </model>
  <run>
    <site>
      <id>288</id>
      <name>Sapelo Island</name>
      <lat>31.40</lat>
      <lon>-81.28</lon>
      <met>$HOME/.pecan/dbfiles/met/31.397Nx81.28W/weather.csv</met>
    </site>
    <start.date>2004-01-01 06:00:00</start.date>
    <end.date>2005-12-31 05:59:00</end.date>
    <host> 
      <name>localhost</name>
    </host>
  </run>
</pecan>

\name{logger.debug}
\alias{logger.debug}
\title{Prints a debug message.}
\usage{
logger.debug(msg, ...)
}
\arguments{
  \item{msg}{the message that should be printed.}

  \item{...}{any additional text that should be printed.}
}
\description{
This function will print a debug message.
}
\examples{
\dontrun{
logger.debug("variable", 5)
}
}
\author{
Rob Kooper
}


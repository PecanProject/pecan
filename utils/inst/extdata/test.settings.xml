<?xml version="1.0"?>
<pecan>
  <pecanDir>$HOME/pecan/</pecanDir>
  <outdir>/tmp/out/</outdir>
  <pfts>
    <pft>
      <name>sipnet.temperate.coniferous</name>
      <outdir>/tmp/out/</outdir>
      <constants>
        <num>1</num>
      </constants>
    </pft>
  </pfts>
  <database>
    <userid>bety</userid>
    <passwd>bety</passwd>
    <host>localhost</host>
    <name>bety</name>
  </database>
  <meta.analysis>
    <iter>3000</iter>
    <random.effects>FALSE</random.effects>
  </meta.analysis>
  <sensitivity.analysis>
    <quantiles>
      <quantile/>
      <sigma>0</sigma>
    </quantiles>
    <start.year>2002</start.year>
    <end.year>2006</end.year>
  </sensitivity.analysis>
  <model>
    <name>SIPNET</name>
    <binary>/usr/local/bin/sipnet.runk</binary>
    <id>9</id>
  </model>
  <run>
    <folder>/tmp/out/</folder>
    <site>
      <id>680</id>
      <name>Young Jack Pine (YJP)</name>
      <lat>42.430000</lat>
      <lon>-84.020000</lon>
      <met>$HOME/sites/clim/yjp.clim</met>
    </site>
    <start.date>2004-01-01 06:00:00</start.date>
    <end.date>2006-01-01 05:59:00</end.date>
    <host>
      <name>localhost</name>
      <rundir>/tmp/out/</rundir>
      <outdir>/tmp/out/</outdir>
      <ed>
      </ed>
    </host>
  </run>
</pecan>
    </pft>
  </pfts>
  
  <config.header>
    <radiation>
      <lai_min>0.01</lai_min>
    </radiation>
    <ed_misc>
      <output_month>12</output_month>      
    </ed_misc> 
  </config.header>
  
  <meta.analysis>
    <iter>50000</iter>
    <random.effects>TRUE</random.effects>
  </meta.analysis>
  
  <ensemble>
    <size>100</size>
  </ensemble>

  <sensitivity.analysis>
    <quantiles>
      <quantile></quantile>
      <sigma>-3</sigma>
      <sigma>-2</sigma>
      <sigma>-1</sigma>
      <sigma>1</sigma>
      <sigma>2</sigma>
      <sigma>3</sigma>
    </quantiles>
    <start.year>2004</start.year>
    <end.year>2006</end.year>
  </sensitivity.analysis>

  
  <data.assimilation>
    <nmcmc>1000</nmcmc>
  </data.assimilation>
      
  <pecanDir>/home/dlebauer/pecan/</pecanDir>
  <Rlibdir>/home/dlebauer/lib/R/</Rlibdir>
  <outdir>/home/dlebauer/pecan/test/out/</outdir>
  <database>

<?xml version="1.0"?>
<pecan>
  <pfts>
    <pft>
       <name>tundra.evergreen</name>
       <outdir>~/pecan/out/evergreen/</outdir>
       <edin>~/pecan/edin/templateED2IN-tundra.evergreen</edin>
       <exclude>
         <trait>c2n_leaf</trait>
       </exclude>
       <constants>
         <num>8</num>
       </constants>
    </pft>
    <pft>
       <name>tundra.deciduous</name>
       <outdir>~/pecan/out/deciduous/</outdir>
       <edin>~/pecan/edin/templateED2IN-tundra.deciduous</edin>
       <exclude>
         <trait>c2n_leaf</trait>
       </exclude>
       <constants>
         <num>11</num>    
         <storage_turnover_rate>0.0</storage_turnover_rate>
       </constants>
    </pft>
    <pft>
      <name>tundra.grass</name>
      <outdir>~/pecan/out/grass/</outdir>
      <edin>~/pecan/edin/templateED2IN-tundra.grass</edin>
      <exclude>
        <trait>c2n_leaf</trait>
      </exclude>
      <constants>
        <num>5</num>    
      </constants>
    </pft>
  </pfts>
  <meta.analysis>
    <iter>300000</iter>
    <random.effects>TRUE</random.effects>
  </meta.analysis>
  <ensemble>
    <size>0</size>
  </ensemble>
  <pecanDir>~/pecan/</pecanDir>
  <Rlibdir>~/lib/R/</Rlibdir>
  <outdir>~/pecan/out/</outdir>
  <sensitivity.analysis>
    <quantiles>
      <quantile></quantile>
      <sigma>-3</sigma>
      <sigma>-2</sigma>
      <sigma>-1</sigma>
      <sigma>1</sigma>
      <sigma>2</sigma>
      <sigma>3</sigma>
    </quantiles>
    <start.year></start.year>
    <end.year></end.year>
  </sensitivity.analysis>
  <database>
    <userid>ebi_analys_user</userid>
    <passwd>b742xsAu</passwd>
    <location>localhost</location>
    <name>ebi_analysis</name>
  </database>
  <run>
    <site>
      <lat></lat>
      <lon></lon>
      <met.data.header></met.data.header>
    </site>
    <start.date>2006/01/01</start.date>
    <end.date>2010/01/01</end.date>
    <host>
      <name>ebi-cluster.igb.uiuc.edu</name>
      <rundir>/home/davids14/EDBRAMS/ED/run/</rundir>
      <outdir>/home/scratch/davids14/pecan/out/</outdir>
    </host> 
  </run>
</pecan>
